#include <iostream>
using namespace std;
class arraycall
{
    public:
void printarray(int **array,int m,int n){
    int i,j;
    cout<<"Print Function Called Array"<<endl;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            cout<<array[i][j]<<" ";
        }
        cout<<endl;
    }
}
};
int main()
{
    int m,n,i,j;
    arraycall ac;
    cout<<"Print values of m and n: ";
    cin>>m>>n;
    int **array=new int*[m];
    for(i=0;i<m;i++){
        array[i]=new int [n];
    }
    cout<<"Print Array"<<endl;
    for(i=0;i<m;i++){
        for(j=0;j<n;j++){
            cout<<" ";
            cin>>array[i][j];
        }
        cout<<endl;
    }
    ac.printarray(array,m,n);
    return 0;
}
